<% if false %>
    <% provide(:title, @title) %>
    <div class="row">
     <aside class="col-md-4">
        <section>
          <%= render 'shared/stats' %>
          <% if @users.any? %>
            <div class="user_avatars">
              <% @users.each do |user| %>
                <%= link_to gravatar_for(user, size: 30), user %>
              <% end %>
            </div>
          <% end %>
        </section>
        
        
     </aside>
    </div>
<% end %>